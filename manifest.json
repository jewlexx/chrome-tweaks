{
  "manifest_version": 3,
  "name": "Chrome Tweaks",
  "version": "0.0.1",
  "description": "A collection of little scripts that I use in my day-to-day",
  "content_scripts": [
    {
      "matches": ["https://www.npmjs.com/package/*"],
      "js": ["npm.js"]
    },
    {
      "matches": ["https://orteil.dashnet.org/*"],
      "js": ["cookie.js"]
    },
    {
      "matches": [
        "https://www.youtube.com/feed/channels",
        "https://www.youtube.com/channel/*",
        "https://www.youtube.com/user/*"
      ],
      "js": ["youtube.js"]
    }
  ],
  "author": "jamesinaxx"
}
